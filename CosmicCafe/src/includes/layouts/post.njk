---
layout: layouts/base.njk
---
<article class="post">
  <header>
    <h1>{{ title }}</h1>
    <p class="meta">{{ date | date("MMMM d, yyyy") }}</p>
    {% if description %}<p class="description">{{ description }}</p>{% endif %}
  </header>
  <div class="content">
    {{ content | safe }}
  </div>
</article>



